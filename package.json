{
  "name": "@gembera/gef-sys",
  "version": "0.9.0",
  "repository": "https://github.com/gembera/gef-sys.git",
  "author": "Xinjie ZHANG <xinjie.zhang@gmail.com>",
  "license": "MIT",
  "scripts": {
    "debug:cmake": "rimraf build/debug && make-dir build/debug && cd build/debug && cross-env CMAKE_BUILD_TYPE=Debug cmake ../..",
    "debug:build": "cd build/debug && cmake --build .",
    "debug:test": "cd build/debug && ctest",
    "release:cmake": "rimraf build/release && make-dir build/release && cd build/release && cmake ../..",
    "release:build": "cd build/release && cmake --build .",
    "release:test": "cd build/release && make ctest",
    "postinstall": "cpr node_modules/@gembera/ pkgs/ -d"
  },
  "devDependencies": {
    "cpr": "^3.0.1",
    "cross-env": "^7.0.3",
    "make-dir-cli": "^3.0.0",
    "rimraf": "^5.0.1"
  },
  "dependencies": {
    "@gembera/gef-core": "^0.9.2"
  }
}
